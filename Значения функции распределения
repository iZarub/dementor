#include <iostream>
#include <vector>
#include<math.h>

using std::cin;
using std::cout;
using std::endl;
using std::vector;



int main() {

	int t = 0;
	double G = 0.05;

	vector<double>initial;
	vector<double>value;

	for (int x = 0; x <= 9; x++) {
		double f = pow(exp(1), (-pow(x, 2) - 5) / 2);
		initial.push_back(f);
	}
	initial.push_back(0);

	for (double& f : initial) {
			cout << f << " ";
	}
	cout << '\n' << '\n';



	while (t <= 100) {
		for (int i = 0; i < 9; i++) {
			value.push_back(initial[i] - G * (initial[i+1] - initial[i]));
		}
		value.push_back(initial[9] - G * (initial[10] - initial[9]));
		value.push_back(0);


		for (double& v : value) {
			cout << v << " " << '\n';	
		}
		initial = value;
		value.clear();
		cout << '\n' << '\n';
		t = t + 1;

	}
	
